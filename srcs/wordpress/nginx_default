error_log                       /var/log/nginx/error.log warn;

server {
		listen 5050;
		listen	[::]:5050;

		root /var/www/wordpress;

		index index.php;

		location ~ \.php$ {
			try_files $uri =404;
			fastcgi_pass      127.0.0.1:9000;
			fastcgi_index     index.php;
			include /etc/nginx/fastcgi.conf;
		}
}
